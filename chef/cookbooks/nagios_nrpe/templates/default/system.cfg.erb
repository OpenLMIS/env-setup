command[check_disk]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -e
command[check_ssh]=/usr/lib64/nagios/plugins/check_ssh <%= node.fqdn %> 
command[check_space]=/usr/lib64/nagios/plugins/check_space
command[check_inode]=/usr/lib64/nagios/plugins/check_inode
command[check_memory]=/usr/lib64/nagios/plugins/check_memory
command[check_procs]=/usr/lib64/nagios/plugins/check_procs
command[check_load]=/usr/lib64/nagios/plugins/check_load -w <%= node.cpu.count+10 %> -c <%= node.cpu.count+15 %>
